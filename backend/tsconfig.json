{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es6",
    "allowJs": false,
    "checkJs": false,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": false,
    "sourceMap": true,
    "baseUrl": "."
  },
  "include": ["src"]
}
